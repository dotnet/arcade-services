{
    "KeyVaultUri": "https://helixstagingservice.vault.azure.net/",
    "AppConfigurationUri": "https://helixstagingservice.azconfig.io/",
    "BuildResultAnalysisStorageQueue": {
        "Name": "buildresultanalysis-localdev"
    },
    "GitHubAppAuth": {
        "GitHubAppId": "[vault(local-build-analysis-github-app-id)]",
        "PrivateKey": "[vault(local-build-analysis-github-app-private-key)]"
    },
    "BuildConfigurationFileSettings": {
        "FileName": "build-configuration-development.json"
    },
    "Parallelism": {
        "WorkerCount": 1
    },
    "GitHubIssuesSettings": {
        "CriticalIssuesRepositories": [
            "maestro-auth-test/build-result-analysis-test",
            "maestro-auth-test/build-result-analysis-test"
        ],
        "CriticalIssuesLabels": [
            "Critical",
            "FC - Infrastructure"
        ],
        "KnownIssuesRepositories": [
            "maestro-auth-test/build-result-analysis-test",
            "maestro-auth-test/build-result-analysis-test"
        ],
        "KnownIssuesLabels": [
            "Known Build Error",
            "blocking-clean-ci"
        ]
    },
    "Sql": {
        "DataSource": "Data Source=helixdata-int.database.windows.net;Authentication=Active Directory Default;Database=helixdata-int;Encrypt=true"
    },
    "KnownIssuesKustoOptions": {
        "KustoClusterUri": "https://engdata.westus2.kusto.windows.net",
        "KustoIngestionUri": "https://ingest-engdata.westus2.kusto.windows.net",
        "UseAzCliAuthentication": true
    }
}
