{
    "Logging": {
        "LogLevel": {
            "Default": "Information",

            "Azure.Core": "Warning",
            "Azure.Identity": "Warning",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
            "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
            "Microsoft.IdentityModel.LoggingExtensions.IdentityLoggerAdapter": "Warning",
            "Microsoft.DotNet.GitHub.Authentication": "Warning"
        }
    },
    "AllowedHosts": "*",
    "WorkItemQueueName": "build-insights-workitems",
    "WorkItemConsumerCount": 4,
    "WorkItemConsumerOptions": {
        "QueuePollTimeout": "00:01:00",
        "MaxWorkItemRetries": 3,
        "QueueMessageInvisibilityTime": "00:15:00"
    },
    "EntraAuthentication": {
        "Instance": "https://login.microsoftonline.com/",
        "TenantId": "72f988bf-86f1-41af-91ab-2d7cd011db47",
        "UserRole": "User",
        "AdminRole": "Admin",
        "CallbackPath": "/signin-oidc",
        "SignedOutCallbackPath": "/signout-callback-oidc"
    },
    "KnownIssuesProject": {
        "KnownIssueLabels": [ "Known Build Error", "blocking-clean-ci" ],
        "CriticalIssueLabels": [ "Critical" ],
        "IssueTypeField": "Known Issue Type"
    },
    "KnownIssuesCreation": {
        "InfrastructureIssueParameters": {
            "Labels": [ "Known Build Error", "First Responder", "Detected By - Customer" ],
            "GithubTemplateName": "z-build-break-infrastructure-issue-template.yml",
            "Repository": "dotnet/dnceng"
        },
        "RepositoryIssueParameters": {
            "Labels": [ "Known Build Error", "blocking-clean-ci" ]
        }
    },
    "KnownIssuesAnalysisLimits": {
        "RecordCountLimit": 1000,
        "LogLinesCountLimit": 100000,
        "FailingTestCountLimit": 1000,
        "HelixLogsFilesLimit": 100
    },
    "KnownIssuesKustoOptions": {
        "Database": "engineeringdata"
    }
}
