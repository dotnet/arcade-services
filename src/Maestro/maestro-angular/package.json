{
  "name": "maestro-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "pree2e": "cd ../Maestro.Web && dotnet build",
    "run-dotnet": "cd ../Maestro.Web && dotnet run --no-build",
    "run-e2e": "ng e2e",
    "e2e": "concurrently --kill-others -s first \"npm:run-e2e\" \"npm:run-dotnet\"",
    "postbuild": "node post-build.js && webpack-bundle-analyzer dist/maestro-angular/stats.json -m static -O -r dist/maestro-angular/bundle-report.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.15",
    "@angular/common": "~7.2.15",
    "@angular/compiler": "~7.2.15",
    "@angular/core": "~7.2.15",
    "@angular/forms": "~7.2.15",
    "@angular/platform-browser": "~7.2.15",
    "@angular/platform-browser-dynamic": "~7.2.15",
    "@angular/router": "~7.2.15",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@microsoft/applicationinsights-web": "^2.0.0-rc3",
    "@ng-bootstrap/ng-bootstrap": "^4.0.4",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "dagre": "^0.8.5",
    "dagre-d3": "^0.6.4",
    "date-fns": "^2.0.0-alpha.27",
    "jquery": "^3.4.1",
    "ngx-pagination": "^3.2.1",
    "popper.js": "^1.15.0",
    "rxjs": "^6.4.0",
    "tipsy": "^1.0.0",
    "tslib": "^1.9.0",
    "viz.js": "^2.1.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/dagre": "^0.7.42",
    "@types/dagre-d3": "^0.4.39",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^5.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "^5.4.2",
    "puppeteer": "^2.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
