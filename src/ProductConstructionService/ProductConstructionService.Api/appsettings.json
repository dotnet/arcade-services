{
    "GitHub": {
        "GitHubAppId": "[vault(github-app-id)]",
        "PrivateKey": "[vault(github-app-private-key)]"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "JobConsumerOptions": {
        "JobQueueName": "pcs-jobs",
        "QueuePollTimeout": "00:01:00",
        "MaxJobRetries": 3,
        "QueueMessageInvisibilityTime": "00:01:00"
    }
}
